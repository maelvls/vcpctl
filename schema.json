{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "$id": "https://example.com/certificate-config.schema.json",
  "title": "Firefly Configuration",
  "description": "Schema for the Firefly configuration. Inspired from: https://api.venafi.cloud/v3/api-docs/vcamanagement-service",
  "$ref": "#/$defs/ExtendedConfigurationInformation",
  "$defs": {
    "AnyValue": {
      "description": "Can be any value - string, number, boolean, array or object."
    },
    "AdvancedSettingsInformation": {
      "type": "object",
      "properties": {
        "enableIssuanceAuditLog": {
          "type": "boolean",
          "description": "Whether audit log entries must be generated for each issued certificate",
          "example": false
        },
        "includeRawCertDataInAuditLog": {
          "type": "boolean",
          "description": "Whether the raw certificate data must be included in the audit log entry",
          "example": false
        },
        "requireFIPSCompliantBuild": {
          "type": "boolean",
          "description": "Whether FIPS-compliant build is required",
          "example": false
        }
      }
    },
    "AwsCloudProviderInformation": {
      "required": ["accountIds", "regions"],
      "type": "object",
      "properties": {
        "accountIds": {
          "type": "array",
          "description": "Array of AWS account IDs each of which should be a 12-digit identifier",
          "example": 123456789012,
          "items": {
            "type": "string",
            "description": "Array of AWS account IDs each of which should be a 12-digit identifier",
            "example": "123456789012"
          }
        },
        "regions": {
          "type": "array",
          "description": "Array of AWS regions",
          "example": "us-west-1",
          "items": {
            "type": "string",
            "description": "Array of AWS regions",
            "example": "us-west-1",
            "enum": [
              "us-east-1",
              "us-east-2",
              "us-west-1",
              "us-west-2",
              "af-south-1",
              "ap-east-1",
              "ap-south-2",
              "ap-southeast-3",
              "ap-southeast-4",
              "ap-south-1",
              "ap-northeast-3",
              "ap-northeast-2",
              "ap-southeast-1",
              "ap-southeast-2",
              "ap-northeast-1",
              "ca-central-1",
              "eu-central-1",
              "eu-west-1",
              "eu-west-2",
              "eu-south-1",
              "eu-west-3",
              "eu-south-2",
              "eu-north-1",
              "eu-central-2",
              "me-south-1",
              "me-central-1",
              "sa-east-1",
              "us-gov-east-1",
              "us-gov-west-1"
            ]
          }
        }
      }
    },
    "AzureCloudProviderInformation": {
      "required": ["subscriptionIds"],
      "type": "object",
      "properties": {
        "subscriptionIds": {
          "type": "array",
          "description": "Array of Azure subscription IDs each of which should be UUID",
          "example": "8d10da13-8125-4ba9-a717-bf7490507b3d",
          "items": {
            "type": "string",
            "description": "Array of Azure subscription IDs each of which should be UUID",
            "format": "uuid",
            "example": "8d10da13-8125-4ba9-a717-bf7490507b3d"
          }
        }
      }
    },
    "ClientAuthenticationInformation": {
      "required": ["type"],
      "type": "object",
      "properties": {
        "type": {
          "type": "string"
        }
      },
      "discriminator": {
        "propertyName": "type"
      },
      "oneOf": [
        {
          "$ref": "#/$defs/JwtJwksAuthenticationInformation"
        },
        {
          "$ref": "#/$defs/JwtOidcAuthenticationInformation"
        }
      ]
    },
    "ClientAuthorizationInformation": {
      "type": "object",
      "properties": {
        "customClaimsAliases": {
          "$ref": "#/$defs/CustomClaimsAliasesInformation"
        }
      }
    },
    "CloudProvidersInformation": {
      "type": "object",
      "properties": {
        "aws": {
          "$ref": "#/$defs/AwsCloudProviderInformation"
        },
        "azure": {
          "$ref": "#/$defs/AzureCloudProviderInformation"
        },
        "google": {
          "$ref": "#/$defs/GoogleCloudProviderInformation"
        }
      }
    },
    "CustomClaimsAliasesInformation": {
      "type": "object",
      "properties": {
        "configuration": {
          "maxLength": 128,
          "minLength": 0,
          "type": "string"
        },
        "allowAllPolicies": {
          "maxLength": 128,
          "minLength": 0,
          "type": "string"
        },
        "allowedPolicies": {
          "maxLength": 128,
          "minLength": 0,
          "type": "string"
        }
      }
    },
    "ExtendedConfigurationInformation": {
      "type": "object",
      "properties": {
        "id": {
          "type": "string",
          "description": "UUID of the configuration",
          "format": "uuid",
          "example": "7268d820-a08d-11ed-bbc0-252385d6d389"
        },
        "companyId": {
          "type": "string",
          "description": "UUID specific to your company",
          "format": "uuid",
          "example": "03eb6e61-9806-11ed-84f2-c747fb71e467"
        },
        "name": {
          "type": "string",
          "description": "Name of the configuration",
          "example": "Some configuration"
        },
        "clientAuthentication": {
          "$ref": "#/$defs/ClientAuthenticationInformation"
        },
        "clientAuthorization": {
          "$ref": "#/$defs/ClientAuthorizationInformation"
        },
        "cloudProviders": {
          "$ref": "#/$defs/CloudProvidersInformation"
        },
        "serviceAccountIds": {
          "type": "array",
          "items": {
            "type": "string",
            "format": "uuid"
          }
        },
        "minTlsVersion": {
          "type": "string",
          "description": "Minimum required TLS protocol version",
          "enum": ["TLS12", "TLS13"]
        },
        "advancedSettings": {
          "$ref": "#/$defs/AdvancedSettingsInformation"
        },
        "longLivedCertCount": {
          "type": "integer",
          "description": "Number of long lived certificates",
          "format": "int64",
          "example": 2
        },
        "shortLivedCertCount": {
          "type": "integer",
          "description": "Number of short lived certificates",
          "format": "int64",
          "example": 20
        },
        "ultraShortLivedCertCount": {
          "type": "integer",
          "description": "Number of ultra short lived certificates",
          "format": "int64",
          "example": 200
        },
        "creationDate": {
          "type": "string",
          "description": "When the configuration was initially created",
          "example": "2022-10-10T14:50:41.710+00:00"
        },
        "modificationDate": {
          "type": "string",
          "description": "When the configuration was last modified",
          "example": "2023-12-12T20:00:10.500+00:00"
        },
        "subCaProvider": {
          "$ref": "#/$defs/SubCaProviderInformation"
        },
        "policies": {
          "type": "array",
          "items": {
            "$ref": "#/$defs/PolicyInformation"
          }
        }
      }
    },
    "GoogleCloudProviderInformation": {
      "required": ["projectIdentifiers", "regions"],
      "type": "object",
      "properties": {
        "projectIdentifiers": {
          "type": "array",
          "description": "Array of Google project identifiers each of which should be a string with int64 number or 6 to 30 lowercase letters, digits, or hyphens, should start with a letter and not contain trailing hyphens",
          "example": "415104041262 or tokyo-rain-123",
          "items": {
            "type": "string",
            "description": "Array of Google project identifiers each of which should be a string with int64 number or 6 to 30 lowercase letters, digits, or hyphens, should start with a letter and not contain trailing hyphens",
            "example": "415104041262 or tokyo-rain-123"
          }
        },
        "regions": {
          "type": "array",
          "description": "Array of Google regions",
          "example": "us-west1-a",
          "items": {
            "type": "string",
            "description": "Array of Google regions",
            "example": "us-west1-a",
            "enum": [
              "asia-east1",
              "asia-east2",
              "asia-northeast1",
              "asia-northeast2",
              "asia-northeast3",
              "asia-south1",
              "asia-south2",
              "asia-southeast1",
              "asia-southeast2",
              "australia-southeast1",
              "australia-southeast2",
              "europe-central2",
              "europe-north1",
              "europe-southwest1",
              "europe-west1",
              "europe-west12",
              "europe-west2",
              "europe-west3",
              "europe-west4",
              "europe-west6",
              "europe-west8",
              "europe-west9",
              "me-central1",
              "me-west1",
              "northamerica-northeast1",
              "northamerica-northeast2",
              "southamerica-east1",
              "southamerica-west1",
              "us-central1",
              "us-east1",
              "us-east4",
              "us-east5",
              "us-south1",
              "us-west1",
              "us-west2",
              "us-west3",
              "us-west4"
            ]
          }
        }
      }
    },
    "JwtJwksAuthenticationInformation": {
      "required": ["urls"],
      "type": "object",
      "allOf": [
        {
          "type": "object",
          "properties": {
            "urls": {
              "type": "array",
              "description": "Array JWT JWKS urls",
              "example": ["https://jwks.example.com"],
              "items": {
                "type": "string",
                "description": "Array JWT JWKS urls",
                "example": "[\"https://jwks.example.com\"]"
              }
            }
          }
        }
      ]
    },
    "JwtOidcAuthenticationInformation": {
      "required": ["audience", "baseUrl"],
      "type": "object",
      "allOf": [
        {
          "type": "object",
          "properties": {
            "baseUrl": {
              "maxLength": 256,
              "minLength": 0,
              "type": "string",
              "description": "JWT OpenId base URL",
              "example": "[\"https://openid.example.com\"]"
            },
            "audience": {
              "maxLength": 256,
              "minLength": 0,
              "type": "string",
              "description": "OpenId audience",
              "example": "Client1"
            }
          }
        }
      ]
    },
    "KeyAlgorithmInformation": {
      "required": ["allowedValues", "defaultValue"],
      "type": "object",
      "properties": {
        "allowedValues": {
          "type": "array",
          "description": "Key algorithm allowed values",
          "items": {
            "type": "string",
            "enum": [
              "RSA_2048",
              "RSA_3072",
              "RSA_4096",
              "EC_P256",
              "EC_P384",
              "EC_P521",
              "EC_ED25519"
            ]
          }
        },
        "defaultValue": {
          "type": "string",
          "description": "Key algorithm default value",
          "example": "RSA_4096",
          "enum": [
            "RSA_2048",
            "RSA_3072",
            "RSA_4096",
            "EC_P256",
            "EC_P384",
            "EC_P521",
            "EC_ED25519"
          ]
        }
      },
      "description": "Key algorithm"
    },
    "PolicyInformation": {
      "type": "object",
      "properties": {
        "id": {
          "type": "string",
          "description": "UUID of the policy",
          "format": "uuid",
          "example": "8ae92800-b1e0-11ed-859d-b39255f965ee"
        },
        "companyId": {
          "type": "string",
          "description": "UUID specific to your company",
          "format": "uuid",
          "example": "03eb6e61-9806-11ed-84f2-c747fb71e467"
        },
        "name": {
          "type": "string",
          "description": "Name of the policy",
          "example": "Some policy"
        },
        "validityPeriod": {
          "type": "string",
          "description": "ISO8601 Period Format",
          "format": "PnYnMnDTnHnMnS",
          "example": "P30D"
        },
        "subject": {
          "$ref": "#/$defs/SubjectAttributesInformation"
        },
        "sans": {
          "$ref": "#/$defs/SansInformation"
        },
        "keyUsages": {
          "type": "array",
          "description": "Key usages",
          "example": ["keyEncipherment", "digitalSignature"],
          "items": {
            "type": "string",
            "enum": [
              "digitalSignature",
              "nonRepudiation",
              "keyEncipherment",
              "dataEncipherment",
              "keyAgreement",
              "keyCertSign",
              "cRLSign",
              "encipherOnly",
              "decipherOnly"
            ]
          }
        },
        "extendedKeyUsages": {
          "type": "array",
          "description": "Extended key usages",
          "example": ["CLIENT_AUTH", "SERVER_AUTH"],
          "items": {
            "type": "string",
            "enum": [
              "ANY",
              "SERVER_AUTH",
              "CLIENT_AUTH",
              "CODE_SIGNING",
              "EMAIL_PROTECTION",
              "IPSEC_ENDSYSTEM",
              "IPSEC_TUNNEL",
              "IPSEC_USER",
              "TIME_STAMPING",
              "OCSP_SIGNING",
              "DVCS",
              "SBGP_CERT_AA_SERVER_AUTH",
              "SCVP_RESPONDER",
              "EAP_OVER_PPP",
              "EAP_OVER_LAN",
              "SCVP_SERVER",
              "SCVP_CLIENT",
              "IPSEC_IKE",
              "CAPWAP_AC",
              "CAPWAP_WTP",
              "IPSEC_IKE_INTERMEDIATE",
              "SMARTCARD_LOGON"
            ]
          }
        },
        "keyAlgorithm": {
          "$ref": "#/$defs/KeyAlgorithmInformation"
        },
        "creationDate": {
          "type": "string",
          "description": "When the policy was initially created",
          "example": "2022-10-10T14:50:41.710+00:00"
        },
        "modificationDate": {
          "type": "string",
          "description": "When the policy was last modified",
          "example": "2023-12-12T20:00:10.500+00:00"
        }
      }
    },
    "PropertyInformation": {
      "type": "object",
      "properties": {
        "type": {
          "type": "string",
          "enum": ["IGNORED", "FORBIDDEN", "OPTIONAL", "REQUIRED", "LOCKED"]
        },
        "allowedValues": {
          "type": "array",
          "items": {
            "type": "string"
          }
        },
        "defaultValues": {
          "type": "array",
          "items": {
            "type": "string"
          }
        },
        "minOccurrences": {
          "type": "integer",
          "format": "int32"
        },
        "maxOccurrences": {
          "type": "integer",
          "format": "int32"
        }
      }
    },
    "SansInformation": {
      "type": "object",
      "properties": {
        "dnsNames": {
          "$ref": "#/$defs/PropertyInformation"
        },
        "ipAddresses": {
          "$ref": "#/$defs/PropertyInformation"
        },
        "rfc822Names": {
          "$ref": "#/$defs/PropertyInformation"
        },
        "uniformResourceIdentifiers": {
          "$ref": "#/$defs/PropertyInformation"
        }
      },
      "description": "Subject alternative names"
    },
    "SubCaProviderInformation": {
      "type": "object",
      "properties": {
        "id": {
          "type": "string",
          "description": "UUID of the Sub CA provider",
          "format": "uuid",
          "example": "2f3c6030-b1e0-11ed-a3ed-e3dbaf56a746"
        },
        "companyId": {
          "type": "string",
          "description": "UUID specific to your company",
          "format": "uuid",
          "example": "03eb6e61-9806-11ed-84f2-c747fb71e467"
        },
        "name": {
          "type": "string",
          "description": "Name of the Sub CA provider",
          "example": "Some Sub CA provider"
        },
        "caType": {
          "type": "string",
          "description": "Type of CA this Sub CA provider works with",
          "example": "BUILTIN",
          "enum": [
            "MOCKCA",
            "DIGICERT",
            "GLOBALSIGN",
            "BUILTIN",
            "ENTRUST",
            "MICROSOFT",
            "ACME",
            "ZTPKI",
            "GLOBALSIGNMSSL",
            "TPP"
          ]
        },
        "caAccountId": {
          "type": "string",
          "description": "UUID of the CA account used by this Sub CA provider",
          "format": "uuid",
          "example": "4ece3180-b1e0-11ed-862d-ad36b18e787a"
        },
        "caProductOptionId": {
          "type": "string",
          "description": "UUID of the CA product option used by this Sub CA provider",
          "format": "uuid",
          "example": "6b3d8d20-b1e0-11ed-9c2f-953e35982bbd"
        },
        "validityPeriod": {
          "type": "string",
          "description": "ISO8601 Period Format",
          "format": "PnYnMnDTnHnMnS",
          "example": "P30D"
        },
        "commonName": {
          "type": "string",
          "description": "Common name",
          "example": "example.com"
        },
        "organization": {
          "type": "string",
          "description": "Organization",
          "example": "Some organization"
        },
        "organizationalUnit": {
          "type": "string",
          "description": "Organizational unit",
          "example": "Some organizational unit"
        },
        "locality": {
          "type": "string",
          "description": "Locality",
          "example": "San Antonio"
        },
        "stateOrProvince": {
          "type": "string",
          "description": "State or province",
          "example": "Texas"
        },
        "country": {
          "type": "string",
          "description": "Country",
          "example": "US"
        },
        "keyAlgorithm": {
          "type": "string",
          "description": "Key algorithm type",
          "example": "EC_P256",
          "enum": [
            "RSA_2048",
            "RSA_3072",
            "RSA_4096",
            "EC_P256",
            "EC_P384",
            "EC_P521",
            "EC_ED25519"
          ]
        },
        "pkcs11": {
          "$ref": "#/$defs/SubCaProviderPkcs11ConfigurationInformation"
        },
        "creationDate": {
          "type": "string",
          "description": "When the Sub CA provider was initially created",
          "example": "2022-10-10T14:50:41.710+00:00"
        },
        "modificationDate": {
          "type": "string",
          "description": "When the Sub CA provider was last modified",
          "example": "2023-12-12T20:00:10.500+00:00"
        }
      }
    },
    "SubCaProviderPkcs11ConfigurationInformation": {
      "type": "object",
      "properties": {
        "signingEnabled": {
          "type": "boolean",
          "description": "Indicates whether HSM signing is enabled or not",
          "example": true
        },
        "partitionLabel": {
          "type": "string",
          "description": "HSM Partition Label",
          "example": "firefly-hsm-partition"
        },
        "partitionSerialNumber": {
          "pattern": "^[A-Fa-fx0-9]{0,18}$",
          "type": "string",
          "description": "HSM Partition Serial Number",
          "example": "1444210958933"
        },
        "pin": {
          "type": "string",
          "description": "HSM PIN",
          "writeOnly": true,
          "example": "1234"
        },
        "allowedClientLibraries": {
          "type": "array",
          "description": "A collection of strings each of which represents SHA256 hash of an allowed HSM client library",
          "example": [
            "c34d199f2e30bb679cd9b8533b99975465aefe8b023be1b37972f1ab43ab7b2d"
          ],
          "items": {
            "type": "string"
          }
        }
      }
    },
    "SubjectAttributesInformation": {
      "type": "object",
      "properties": {
        "commonName": {
          "$ref": "#/$defs/PropertyInformation"
        },
        "organization": {
          "$ref": "#/$defs/PropertyInformation"
        },
        "organizationalUnit": {
          "$ref": "#/$defs/PropertyInformation"
        },
        "locality": {
          "$ref": "#/$defs/PropertyInformation"
        },
        "stateOrProvince": {
          "$ref": "#/$defs/PropertyInformation"
        },
        "country": {
          "$ref": "#/$defs/PropertyInformation"
        }
      },
      "description": "Subject attributes"
    },
    "ErrorInformation": {
      "type": "object",
      "properties": {
        "code": {
          "type": "integer",
          "format": "int32"
        },
        "message": {
          "type": "string"
        },
        "args": {
          "type": "array",
          "items": {
            "$ref": "#/$defs/AnyValue"
          }
        }
      }
    },
    "RegionInformation": {
      "type": "object",
      "properties": {
        "id": {
          "type": "string",
          "description": "Region id",
          "example": "us-west-2"
        },
        "name": {
          "type": "string",
          "description": "Region name",
          "example": "US West - Oregon"
        }
      }
    },
    "RegionsInformation": {
      "type": "object",
      "properties": {
        "aws": {
          "type": "array",
          "items": {
            "$ref": "#/$defs/RegionInformation"
          }
        },
        "google": {
          "type": "array",
          "items": {
            "$ref": "#/$defs/RegionInformation"
          }
        }
      }
    },
    "ConfigurationInformation": {
      "type": "object",
      "properties": {
        "id": {
          "type": "string",
          "description": "UUID of the configuration",
          "format": "uuid",
          "example": "7268d820-a08d-11ed-bbc0-252385d6d389"
        },
        "companyId": {
          "type": "string",
          "description": "UUID specific to your company",
          "format": "uuid",
          "example": "03eb6e61-9806-11ed-84f2-c747fb71e467"
        },
        "name": {
          "type": "string",
          "description": "Name of the configuration",
          "example": "Some configuration"
        },
        "clientAuthentication": {
          "$ref": "#/$defs/ClientAuthenticationInformation"
        },
        "clientAuthorization": {
          "$ref": "#/$defs/ClientAuthorizationInformation"
        },
        "cloudProviders": {
          "$ref": "#/$defs/CloudProvidersInformation"
        },
        "serviceAccountIds": {
          "type": "array",
          "items": {
            "type": "string",
            "format": "uuid"
          }
        },
        "minTlsVersion": {
          "type": "string",
          "description": "Minimum required TLS protocol version",
          "enum": ["TLS12", "TLS13"]
        },
        "advancedSettings": {
          "$ref": "#/$defs/AdvancedSettingsInformation"
        },
        "longLivedCertCount": {
          "type": "integer",
          "description": "Number of long lived certificates",
          "format": "int64",
          "example": 2
        },
        "shortLivedCertCount": {
          "type": "integer",
          "description": "Number of short lived certificates",
          "format": "int64",
          "example": 20
        },
        "ultraShortLivedCertCount": {
          "type": "integer",
          "description": "Number of ultra short lived certificates",
          "format": "int64",
          "example": 200
        },
        "creationDate": {
          "type": "string",
          "description": "When the configuration was initially created",
          "example": "2022-10-10T14:50:41.710+00:00"
        },
        "modificationDate": {
          "type": "string",
          "description": "When the configuration was last modified",
          "example": "2023-12-12T20:00:10.500+00:00"
        }
      }
    },
    "IntermediateCertificateInformation": {
      "type": "object",
      "properties": {
        "id": {
          "type": "string",
          "description": "UUID of the intermediate certificate",
          "format": "uuid",
          "example": "265f6d30-a0bb-11ed-aee0-89fe7e5f5d0c"
        },
        "companyId": {
          "type": "string",
          "description": "UUID specific to your company",
          "format": "uuid",
          "example": "03eb6e61-9806-11ed-84f2-c747fb71e467"
        },
        "configuration": {
          "$ref": "#/$defs/ConfigurationInformation"
